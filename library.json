    "url": "https://github.com/nkaaf/Arduino-TemperatureLibrary"
  },
  "authors": {
    "name": "Niklas Kaaf",
    "email": "nkaaf@protonmail.com",
    "url": "https://github.com/nkaaf",
    "maintainer": true
  },
  "license": "LGPL-2.1-only",
  "frameworks": "*",
  "platforms": "*",
  "headers": "Temperature.hpp",
  "examples": [
    {
      "name": "AVRInternalTemperatureSensor",
      "base": "examples/TemperatureLibrary/AVRInternalTemperatureSensor",
